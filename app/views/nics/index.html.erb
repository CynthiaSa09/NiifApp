<p id="notice"><%= notice %></p>

<h1>Nics</h1>


<table class="container">
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nics.each do |nic| %>
      <tr>
        <td><%= nic.number %>.- <%= nic.name %></td>
        <td><%= nic.category.name %></td>
        <td><%= link_to 'Show', nic %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


