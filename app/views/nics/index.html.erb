<p id="notice"><%= notice %></p>

<h1>Nics</h1>


<form class="form-inline-center">
  <input class="form-control mr-sm-2" type="search" placeholder="Search"
  aria-label="Search" name="content">
  <button class="btn btn-outline-danger my-2 my-ms-5"
  type="submit">Search</button>
</form>

<table class="container">
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nics.each do |nic| %>
      <tr>
        <td><%= nic.name %></td>
        <td><%= nic.number %></td>
        <td><%= nic.category_id %></td>
        <td><%= link_to 'Show', nic %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


