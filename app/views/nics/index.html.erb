<p id="notice"><%= notice %></p>

<h1 class="container first">Nics</h1>

<table class="container text-center">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Categoria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nics.each do |nic| %>
      <tr>
        <td><%= nic.number %>.- <%= nic.name %></td>
        <td><%= nic.category.name %></td>
        <td><%= link_to 'Mirar', nic, class: "btn btn-outline-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


