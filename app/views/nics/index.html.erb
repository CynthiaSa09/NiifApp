<p id="notice"><%= notice %></p>

<h1>Nics</h1>

<table class="container">
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th>Text</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nics.each do |nic| %>
      <tr>
        <td><%= nic.name %></td>
        <td><%= nic.number %></td>
        <td><%= nic.text %></td>
        <td><%= nic.category_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


